clc;
 clear all;
 close all;
 n=1000;
 mu=0;
 sigma=1;
 y=normrnd(mu,sigma,1,n);
 Gy=periodogram(y);
 Ry=abs(ifft(Gy,256));
 Ry=[Ry(130:256)' Ry(1:129)'];
 t=-127:1:128;
 figure
 subplot(1,2,1)
 plot(Gy)
 xlabel('Frequency Samples')
 title('PSD')
 subplot(1,2,2)
 stem(t,Ry);
 xlabel('Time Shift')
 title('Autocorrelation')
